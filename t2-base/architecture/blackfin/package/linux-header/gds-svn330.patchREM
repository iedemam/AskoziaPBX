+				retval = PHY_SWITCH_DEFAULT_ADVERTISE;
+				break;
+			case MII_LPA:			/* Link partner ability reg    */
+				retval = PHY_SWITCH_DEFAULT_LPA;
+				break;
+			default :
+				// nicht simulieren 
+				break;
+		}
+	}
+
+	if(!retval)
+		retval = mdiobus_real_read(bus, phy_addr, regnum);
+	
+	return retval;
