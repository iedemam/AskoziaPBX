#Description: AskoziaPBX - Pika Warp

# select init
SDECFGSET_INIT='busybox'

# architecture
SDECFGSET_ARCH="powerpc"
SDECFGSET_POWERPC_ISA="powerpc"
SDECFG_POWERPC_OPT="440fp"

# firmware type
SDECFGSET_IMAGE='ubootimage'

# kernel options
SDECFGSET_PKG_LINUX_CUSTCONFIG_FILE="target/$target/linux.config"
SDECFGSET_PKG_LINUX_CONFIG_STYLE='none'
SDECFGSET_PKG_LINUX_CUSTCONFIG='1'
SDECFGSET_PKG_LINUX_USE26MODTOOLS='1'
SDECFGSET_PKG_LINUX_ACPI_INITRD='0'
SDECFGSET_PKG_LINUX_KEEP_SRC='1'

# busybox package options
SDECFGSET_PKG_BUSYBOX_SYMLINKS='1'

# optimize for size
SDECFGSET_OPT='size'
SDECFGSET_CREATE_DOCS=0

# no package creation
SDECFGSET_PKGFILE_TYPE='none'

# libc + gcc settings
SDECFGSET_LIBC='uclibc'
SDECFGSET_PKG_GCC_JAVA='0'

# do not execute rebuild 'paranoia' stage
SDECFGSET_DO_REBUILD_STAGE=0

# force cross compiling
SDECFGSET_CROSSBUILD=1

# enable native language support
SDECFGSET_DISABLE_NLS=0
